;;; Code:
(defun assassin-langs--activate-lang (lang)
  (funcall (intern (format "assassin-langs--%s-activate" lang))))

(defun assassin-langs-init (langs)
  (mapcar 'assassin-langs--activate-lang langs))  


(provide 'assassin-langs)
